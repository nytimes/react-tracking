    "eslint-plugin-react-hooks": "^2.5.1",
    "husky": "^3.1.0",
    "jest": "^24.9.0",
    "lint-staged": "^9.5.0",
    "prettier": "^1.19.1",
    "prop-types": "^15.7.2",
    "raf-polyfill": "^1.0.0",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-test-renderer": "^16.14.0",
